ul.issue-list.small-block-grid-3.large-block-grid-6
  each fulfillment in fulfillments
    if fulfillment.issue
      with fulfillment.issue as issue
        li class=fulfillment.isMultiple
          link-to 'issue' issue
            .title=issue.title
            .number=issue.number
            if fulfillment.isMultiple
              .count Ã— #{fulfillment.count}
    else
      li.future
  else
    p None
